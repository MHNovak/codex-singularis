    <selectionEntry id="66b0-4847-a04e-0375" name="Regiment of Renown - The Coven of Thryx" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="1c0d-e485-85ff-d367" type="max"/>
